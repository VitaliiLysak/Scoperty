{
  "name": "scoperty",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "stylelint": "./node_modules/.bin/stylelint",
    "svgo": "./node_modules/.bin/svgo",
    "start": "ng serve",
    "start:dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve\n",
    "start:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --prod=true\n",
    "build": "ng build",
    "build:dev": "ng build --configuration=staging",
    "build:stage": "ng build --configuration=staging",
    "build:prod": "ng build --configuration=staging",
    "lint": "ng lint",
    "lint:style": "stylelint",
    "fix:style": "stylelint --fix",
    "test": "ng test",
    "test:ci": "ng test --no-watch --code-coverage",
    "e2e": "ng e2e",
    "storybook:start": "./node_modules/.bin/start-storybook -p 6006",
    "storybook:build": "./node_modules/.bin/build-storybook",
    "responsive-image:generate": "node generate-responsive-image",
    "web-image:generate": "node generate-web-image",
    "svg-image:fix": "svgo -f ./src/assets/icons --enable=prefixIds"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint",
      "git add"
    ],
    "*.css": [
      "npm run fix:style",
      "git add"
    ]
  },
